        <field name="state">code</field>
        <field name="code">
            for record in records:
                record.write({'application_state': 'welcome', 'welcome_letter_date': datetime.date.today()})
        </field>
    </record>    
    <!-- Action: Send Orientation Notice ////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_orientation_notice" model="ir.actions.server">
        <field name="name">Send Orientation Notice</field>
        <field name="model_id" ref="model_members_management_member"/>
        <field name="state">code</field>
        <field name="code">
            for record in records:
                record.write({'application_state': 'orientation', 'orientation_notice_date': datetime.date.today()})
        </field>
    </record>  
    <!-- Action: Send Bylaws ///////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_bylaws" model="ir.actions.server">
        <field name="name">Send Bylaws</field>
        <field name="model_id" ref="model_members_management_member"/>
        <field name="state">code</field>
        <field name="code">
            for record in records:
                record.write({'application_state': 'bylaws', 'bylaws_sent_date': datetime.date.today()})
        </field>
    </record>             
    <!-- Action: Quote Sent ////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_quote_sent" model="ir.actions.server">
        <field name="name">Mark as Quote Sent</field>
        <field name="model_id" ref="model_members_management_member"/>
        <field name="state">code</field>
        <field name="code">
            for record in records:
                record.write({'application_state': 'quote_sent', 'quote_sent_date': datetime.date.today()})
        </field>
    </record>
    <!-- Action: Quote Signed //////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_quote_signed" model="ir.actions.server">
        <field name="name">Mark as Quote Signed</field>
        <field name="model_id" ref="model_members_management_member"/>
        <field name="state">code</field>
        <field name="code">
            for record in records:
                record.write({'application_state': 'quote_signed', 'quote_signed_date': datetime.date.today()})
        </field>
    </record>
    <!-- Action: Subscription Initialized ///////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_sub_initialized" model="ir.actions.server">
        <field name="name">Mark as Subscription Initialized</field>
        <field name="model_id" ref="model_members_management_member"/>
        <field name="state">code</field>
        <field name="code">
            for record in records:
                record.write({'application_state': 'sub_initialized', 'sub_initialized_date': datetime.date.today()})
