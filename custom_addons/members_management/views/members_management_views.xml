<odoo>

    <!-- <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< Member Views >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->

    <!-- Members Form View ///////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="view_members_management_form" model="ir.ui.view">
        <field name="name">members.management.member.form</field>
        <field name="model">members.management.member</field>
        <field name="arch" type="xml">
            <form string="Member">
                <div class="o_form_sheet_bg">
                    <sheet>
                        <!-- Personal Information Group -->
                        <group string="Personal Information">
                            <group>
                                <field name="first_name"/>
                                <field name="last_name"/>
                                <field name="other_names"/>
                                <field name="date_of_birth"/>
                                <field name="marital_status"/>
                                <field name="spouse_name"/>
                            </group>
                            <group>
                                <field name="home_number"/>
                                <field name="mobile_number"/>
                                <field name="email"/>
                                <field name="street_address"/>
                                <field name="street_address2"/>
                                <field name="city"/>
                                <field name="zip"/>
                                <field name="country_id"/>
                            </group>
                            <group>
                                <field name="profile_picture" widget="image" class="oe_avatar" options="{'preview_image': 'profile_picture', 'size': [90, 90]}"/>
                            </group>
                        </group>

                        <!-- Business Information Group -->
                        <group string="Business Information">
                            <group>
                                <field name="business_name"/>
                                <field name="business_street_address"/>
                                <field name="business_city"/>
                                <field name="business_country_id"/>
                            </group>
                            <group>
                                <field name="occupation"/>
                                <field name="nature_of_business" widget="text"/>
                            </group>
                        </group>

                        <!-- Membership Information Group -->
                        <group string="Membership Information">
                            <group>
                                <field name="membership_type"/>
                            </group>
                            <group>
                                <field name="status"/>
                                <field name="member_since"/>
                            </group>
                        </group>

                        <!-- Application Process Log Tab -->
                        <notebook>
                            <!-- Children Tab -->
                            <page string="Children">
                                <field name="children_ids">
                                    <tree>
                                        <field name="first_name"/>
                                        <field name="last_name"/>
                                        <field name="date_of_birth"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <field name="first_name"/>
                                            <field name="last_name"/>
                                            <field name="date_of_birth"/>
                                        </group>
                                    </form>
                                </field>
                            </page>

                            <!-- Documents Tab -->
                            <page string="Documents">
                                <field name="document_ids">
                                    <tree>
                                        <field name="name"/>
                                        <field name="file"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <field name="name"/>
                                            <field name="file"/>
                                        </group>
                                    </form>
                                </field>
                            </page>

                            <!-- Boats Tab -->
                            <page string="Boats">
                                <field name="boat_ids">
                                    <tree>
                                        <field name="name"/>
                                        <field name="size"/>
                                        <field name="engine"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <field name="name"/>
                                            <field name="size"/>
                                            <field name="engine"/>
                                            <field name="partner_ids"/>
                                        </group>
                                    </form>
                                </field>
                            </page>

                            <!-- Other Memberships Tab -->
                            <page string="Other Memberships">
                                <field name="other_memberships_ids">
                                    <tree>
                                        <field name="organization"/>
                                        <field name="role"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <field name="organization"/>
                                            <field name="role"/>
                                        </group>
                                    </form>
                                </field>
                            </page>

                            <page string="Application Process Log">
                                <group>
                                    <group>
                                        <field name="application_state" readonly="1"/>                                    
                                        <field name="application_date" readonly="1"/>
                                        <field name="pdf_application_creation_date" readonly="1"/>
                                        <field name="signed_application_form" widget="binary" readonly="1"/>
                                        <field name="signed_application_form_filename" readonly="1"/>
                                        <field name="acknowledgment_date" readonly="1"/>
                                        <field name="interview_notice_date" readonly="1"/>
                                        <field name="interview_date" readonly="1"/>
                                    </group>
                                    <group>
                                        <field name="followup_date" readonly="1"/>
                                        <field name="welcome_letter_date" readonly="1"/>
                                        <field name="orientation_notice_date" readonly="1"/>
                                        <field name="orientation_date" readonly="1"/>
                                        <field name="bylaws_sent_date" readonly="1"/>
                                    </group>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </div>

                <!-- Chatter -->
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                    <field name="activity_ids"/>
                </div>
            </form>
        </field>
    </record>
    <!-- Members Tree View ///////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="view_members_management_tree" model="ir.ui.view">
        <field name="name">members.management.member.tree</field>
        <field name="model">members.management.member</field>
        <field name="arch" type="xml">
            <tree string="Members" create="false">
                <field name="first_name"/>
                <field name="last_name"/>
                <field name="email"/>
                <field name="mobile_number"/>
                <field name="status"/>
                <field name="subscription_state"/>
            </tree>
        </field>
    </record>   
    <!-- Action for Active Members //////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_active_members" model="ir.actions.act_window">
        <field name="name">Active Members</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_members_management_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_members_management_form')})]"/>
        <field name="domain">[('status', '=', 'member'), ('subscription_state', '=', 'active')]</field>
    </record>
    <!-- Action for Expiring Members //////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_expiring_members" model="ir.actions.act_window">
        <field name="name">Expiring Members</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_members_management_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_members_management_form')})]"/>
        <field name="domain">[('status', '=', 'member'), ('subscription_state', '=', 'expiring')]</field>
    </record>
    <!-- Action for Expired Members //////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_expired_members" model="ir.actions.act_window">
        <field name="name">Expired Members</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_members_management_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_members_management_form')})]"/>
        <field name="domain">[('status', '=', 'member'), ('subscription_state', '=', 'expired')]</field>
    </record>     


    <!-- <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< Application Views >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->

    <!-- Applicant Form View ///////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="view_applicant_form" model="ir.ui.view">
        <field name="name">members.management.member.applicant.form</field>
        <field name="model">members.management.member</field>
        <field name="arch" type="xml">
            <form string="Applicant">
                <div class="o_form_sheet_bg">
                    <header>
                        <!-- Button: Generate PDF Application Form -->
                         <button name="action_generate_pdf_application_form" type="object" string="Generate PDF Form" class="oe_highlight"/>
                        <!-- Button: Create Invoice -->
                        <button name="action_generate_invoice" string="Create Invoice" type="object" class="oe_highlight" invisible="application_state != 'new'"/>
                        <!-- Button: Acknowledge Application -->
                        <button name="%(action_acknowledge_application)d" type="action" string="Acknowledge Application" class="oe_highlight" invisible="application_state != 'new'"/>
                        <!-- Button: Send Notice of Interview -->
                        <button name="%(action_send_notice_of_interview)d" type="action" string="Send Notice of Interview" class="oe_highlight" invisible="application_state != 'acknowledged'"/>
                        <!-- Button: Send Followup Letter -->
                        <button name="%(action_followup_letter)d" type="action" string="Send Followup Letter" class="oe_highlight" invisible="application_state != 'interview'"/>
                        <!-- Button: Send Welcome Letter -->
                        <button name="%(action_welcome_letter)d" type="action" string="Send Welcome Letter" class="oe_highlight" invisible="application_state != 'followup'"/>
                        <!-- Button: Send Orientation Notice -->
                        <button name="%(action_orientation_notice)d" type="action" string="Send Orientation Notice" class="oe_highlight" invisible="application_state != 'welcomed'"/>
                        <!-- Button: Send Bylaws -->
                        <button name="%(action_bylaws)d" type="action" string="Send Bylaws" class="oe_highlight" invisible="application_state != 'orientation'"/>
                        <!-- Button: Mark as Quote Sent -->
                        <button name="%(action_quote_sent)d" type="action" string="Send Quote" class="oe_highlight" invisible="application_state != 'bylaws'"/>
                        <!-- Button: Mark as Quote Signed -->
                        <button name="%(action_quote_signed)d" type="action" string="Quote Signed" class="oe_highlight" invisible="application_state != 'quote_sent'"/>
                        <!-- Button: Mark as Subscription Initialized -->
                        <button name="%(action_sub_initialized)d" type="action" string="Initialize Subscription" class="oe_highlight" invisible="application_state != 'quote_signed'"/>
                    </header>                
                    <sheet>
                        <!-- Personal Information Group -->
                        <group string="Personal Information">
                            <group>
                                <field name="first_name"/>
                                <field name="last_name"/>
                                <field name="other_names"/>
                                <field name="date_of_birth"/>
                                <field name="marital_status"/>
                                <field name="spouse_name"/>
                            </group>
                            <group>
                                <field name="home_number"/>
                                <field name="mobile_number"/>
                                <field name="email"/>
                                <field name="street_address"/>
                                <field name="street_address2"/>
                                <field name="city"/>
                                <field name="zip"/>
                                <field name="country_id"/>
                            </group>
                            <group>
                                <field name="profile_picture" widget="image" class="oe_avatar" options="{'preview_image': 'profile_picture', 'size': [90, 90]}"/>
                            </group>
                        </group>

                        <!-- Business Information Group -->
                        <group string="Business Information">
                            <group>
                                <field name="business_name"/>
                                <field name="business_street_address"/>
                                <field name="business_city"/>
                                <field name="business_country_id"/>
                            </group>
                            <group>
                                <field name="occupation"/>
                                <field name="nature_of_business" widget="text"/>
                            </group>
                        </group>

                        <!-- Membership Information Group -->
                        <group string="Membership Information">
                            <group>
                                <field name="membership_type"/>
                            </group>
                            <group>
                                <field name="status" readonly="1"/>
                            </group>
                        </group>

                        <!-- Partner Information Group -->
                        <group string="Related Contact">
                            <group>
                                <field name="partner_id" readonly="1"/>
                            </group>
                        </group>                        

                        <!-- Proposer & Seconder Group -->
                        <group string="Proposer and Seconder">
                            <group>
                                <field name="proposer_id" string="Proposer" options="{'no_create': True}"/>
                            </group>
                            <group>
                                <field name="seconder_id" string="Seconder" options="{'no_create': True}"/>
                            </group>
                        </group>

                        <!-- Application Process Log Tab -->
                        <notebook>
                            <page string="Application Process Log">
                                <group>
                                    <group>
                                        <field name="application_state" readonly="1"/>
                                        <field name="application_date" readonly="1"/>
                                        <field name="pdf_application_creation_date" readonly="1"/>
                                        <field name="signed_application_form" widget="binary"/>
                                        <field name="signed_application_form_filename"/>
                                        <field name="signed_application_date" readonly="1"/>
                                        <field name="acknowledgment_date" readonly="1"/>
                                    </group>
                                    <group>
                                        <field name="interview_notice_date" readonly="1"/>
                                        <field name="interview_date" readonly="1"/>                                    
                                        <field name="followup_date" readonly="1"/>
                                        <field name="welcome_letter_date" readonly="1"/>
                                        <field name="orientation_notice_date" readonly="1"/>
                                        <field name="orientation_date" readonly="1"/>
                                        <field name="bylaws_sent_date" readonly="1"/>
                                    </group>
                                </group>
                            </page>  
                            <!-- Invoices Tab -->
                            <page string="Invoices">
                                <field name="partner_id" invisible="1"/>
                                <field name="invoice_ids">
                                    <tree string="Invoice" create="true" edit="true">
                                        <field name="name"/>
                                        <field name="invoice_date"/>
                                        <field name="amount_total"/>
                                        <field name="state"/>
                                    </tree>
                                </field>
                            </page>                                                                           
                            <!-- Children Tab -->
                            <page string="Children">
                                <field name="children_ids">
                                    <tree>
                                        <field name="first_name"/>
                                        <field name="last_name"/>
                                        <field name="date_of_birth"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <field name="first_name"/>
                                            <field name="last_name"/>
                                            <field name="date_of_birth"/>
                                        </group>
                                    </form>
                                </field>
                            </page>

                            <!-- Documents Tab -->
                            <page string="Documents">
                                <field name="document_ids">
                                    <tree>
                                        <field name="name"/>
                                        <field name="file"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <field name="name"/>
                                            <field name="file"/>
                                        </group>
                                    </form>
                                </field>
                            </page>

                            <!-- Boats Tab -->
                            <page string="Boats">
                                <field name="boat_ids">
                                    <tree>
                                        <field name="name"/>
                                        <field name="size"/>
                                        <field name="engine"/>
                                        <field name="partner_ids" invisible="1"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <field name="name"/>
                                            <field name="size"/>
                                            <field name="engine"/>
                                            <field name="partner_ids"/>
                                        </group>
                                    </form>
                                </field>
                            </page>

                            <!-- Other Memberships Tab -->
                            <page string="Other Memberships">
                                <field name="other_memberships_ids">
                                    <tree>
                                        <field name="organization"/>
                                        <field name="role"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <field name="organization"/>
                                            <field name="role"/>
                                        </group>
                                    </form>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </div>

                <!-- Chatter -->
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                    <field name="activity_ids"/>
                </div>
            </form>
        </field>
    </record>
    <!-- Applicant Tree View ///////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="view_applicant_tree" model="ir.ui.view">
        <field name="name">members.management.member.applicant.tree</field>
        <field name="model">members.management.member</field>
        <field name="arch" type="xml">
            <tree string="Applicants">
                <field name="first_name"/>
                <field name="last_name"/>
                <field name="email"/>
                <field name="mobile_number"/>
                <field name="status"/>
            </tree>
        </field>
    </record>

     <!-- Window Actions -->

    <!-- Action for New Applicants /////////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_new_applicants" model="ir.actions.act_window">
        <field name="name">New Applicants</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_applicant_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_applicant_form')})]"/>
        <field name="domain">[('status', '=', 'applicant'), ('application_state', '=', 'new')]</field>
    </record>
    <!-- Action for Acknowledged Applicants ////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_acknowledged_applicants" model="ir.actions.act_window">
        <field name="name">Acknowledged Applicants</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_applicant_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_applicant_form')})]"/>
        <field name="domain">[('status', '=', 'applicant'), ('application_state', '=', 'acknowledged')]</field>
    </record>    
    <!-- Action for Applicants sent Notice of Interview ////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_notice_of_interview_applicants" model="ir.actions.act_window">
        <field name="name">Sent Notice of Interview</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_applicant_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_applicant_form')})]"/>
        <field name="domain">[('status', '=', 'applicant'), ('application_state', '=', 'interview')]</field>
    </record> 
    <!-- Action for Applicants sent Followup Letter ////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_followup_letter_applicants" model="ir.actions.act_window">
        <field name="name">Sent Followup Letter</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_applicant_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_applicant_form')})]"/>
        <field name="domain">[('status', '=', 'applicant'), ('application_state', '=', 'followup')]</field>
    </record>   
    <!-- Action for Applicants sent Welcome Letter //////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_welcome_letter_applicants" model="ir.actions.act_window">
        <field name="name">Sent Welcome Letter</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_applicant_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_applicant_form')})]"/>
        <field name="domain">[('status', '=', 'applicant'), ('application_state', '=', 'welcome')]</field>
    </record>      
    <!-- Action for Applicants sent Orientation Notice //////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_orientation_notice_applicants" model="ir.actions.act_window">
        <field name="name">Sent Orientation Notice</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_applicant_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_applicant_form')})]"/>
        <field name="domain">[('status', '=', 'applicant'), ('application_state', '=', 'orientation')]</field>
    </record> 
    <!-- Action for Applicants sent Bylaws //////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_view_bylaws_applicants" model="ir.actions.act_window">
        <field name="name">Sent Bylaws</field>
        <field name="res_model">members.management.member</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'tree', 'view_id': ref('view_applicant_tree')}), (0, 0, {'view_mode': 'form', 'view_id': ref('view_applicant_form')})]"/>
        <field name="domain">[('status', '=', 'applicant'), ('application_state', '=', 'bylaws')]</field>
    </record>   

    <!-- Server Actions -->

    <!-- Action: Acknowledge Application ////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_acknowledge_application" model="ir.actions.server">
        <field name="name">Acknowledge Application Receipt</field>
        <field name="model_id" ref="model_members_management_member"/>
        <field name="state">code</field>
        <field name="code">
            for record in records:
                record.write({'application_state': 'acknowledged', 'acknowledgment_date': datetime.date.today()})
        </field>
    </record>
    <!-- Action: Send Notice of Interview ////////////////////////////////////////////////////////////////////////////////////////////////// -->
    <record id="action_send_notice_of_interview" model="ir.actions.server">
        <field name="name">Send Notice of Interview</field>
        <field name="model_id" ref="model_members_management_member"/>
        <field name="state">code</field>
        <field name="code">
            for record in records:
                record.write({'application_state': 'interview', 'interview_notice_date': datetime.date.today()})
        </field>
    </record> 
